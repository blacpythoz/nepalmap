#!/bin/bash

# foreground
f=$'\e[34m'
# background
b=$'\e[41m'
# rest
r=$'\e[49m'

cat << eof
${f}                          ▄▄ 
${f}                      ▄▀▀▀  ▀▀▀▄    
${f}               ▄▀▀▄  ▄▀         █  
${f}             ▄▀    ▀▀            ▀▄▄  
